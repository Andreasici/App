<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.routesplanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Routes planner"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.routesplanner.activity.MapsActivity"></activity>
        <activity android:name="com.example.routesplanner.FacebookLogin" />
        <activity android:name="com.example.routesplanner.activity.LoginActivity" />
        <activity android:name="com.example.routesplanner.activity.RegistrationActivity" />
        <activity android:name="com.example.routesplanner.activity.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>